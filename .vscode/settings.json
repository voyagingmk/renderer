// 将设置放入此文件中以覆盖默认值和用户设置。
{
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "*/build/": true,
        "*/temp/": true,
        "/library/*": true,
        "tiancai/": true,
        "**/*.anim": true
    },
    "files.exclude": {
        "**/.git": true,
        "**/.project.js": true,
        "**/.DS_Store": true,
        "**/*.meta": true,
        "**/*.xml": true,
        "**/library/**": true,
        "*/local/": true,
        "tiancai/": true,
        "*/temp/": true,
        "robot/": true
    },
    "editor.minimap.enabled": false,
    "workbench.iconTheme": null,
    "eslint.enable": true,
    "editor.formatOnSave": true,
    "files.associations": {
        "memory": "cpp"
    }
}